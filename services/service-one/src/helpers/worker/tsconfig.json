{
  // "extends": "../tsconfig.json",
  "exclude": ["*"],
  // "exclude": ["node_modules", "out", "*"],
  "include": [],
  "files": ["./rpc-worker.js", "./worker.js", "./server.js", "./actor.js"],

  "compilerOptions": {
    /* Module Resolution Interop Constraints */
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    /* JavaScript Support */
    "allowJs": true,
    "checkJs": false,
    "maxNodeModuleJsDepth": 1,
    "disableSizeLimit": false,

    /* Specify a set of bundled library declaration files that describe the target runtime environment. */
    "lib": ["esnext"],
    /* Specify type package names to be included without being referenced in a source file. */
    "types": ["node"],
    /* Emit Options */
    "emitBOM": false,
    // "rootDir": ".",
    "outDir": "../../../out/worker",
    "declarationDir": "../../../out/worker/typings",
    "tsBuildInfoFile": "../../../out/worker/ts-out-info.lib",
    "generateCpuProfile": "../../../out/worker/performance/cpu-info.cpuprofile",
    "importsNotUsedAsValues": "remove",
    "newLine": "lf",
    "noEmit": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    /* Source Map and declaration options */
    "declaration": true,
    "declarationMap": false,
    "inlineSourceMap": false,
    "inlineSources": false,
    "sourceMap": false,
    "emitDeclarationOnly": false,

    //     "declaration":"",
